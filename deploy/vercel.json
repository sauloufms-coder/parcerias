{
  "version": 2,
  "routes": [
    { "src": "^/login$", "dest": "/login.html" },

    { "src": "^/$", "has": [{ "type": "cookie", "key": "sess", "value": "ok" }], "dest": 
"/app/index.html" },
    { "src": "^/app$", "has": [{ "type": "cookie", "key": "sess", "value": "ok" }], 
"dest": "/app/index.html" },
    { "src": "^/app/(.*)$", "has": [{ "type": "cookie", "key": "sess", "value": "ok" }], 
"dest": "/app/$1" },

    {
      "src": "^(?!/(api|login|app|icons|favicon\\.png|manifest\\.json)).*$",
      "missing": [{ "type": "cookie", "key": "sess", "value": "ok" }],
      "dest": "/login.html"
    }
  ]
}

